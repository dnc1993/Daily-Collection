{
  "version": 2,
  "routes": [
    {
      "src": "/api/loans",
      "dest": "/api/loans.py"
    },
    {
      "src": "/api/loans/(.*)",
      "dest": "/api/loans.py"
    },
    {
      "src": "/api/payments/?(.*)",
      "dest": "/api/payments.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
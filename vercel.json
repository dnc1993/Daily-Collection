{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "framework": null,
  "routes": [
    {
      "src": "/api/loans",
      "dest": "/api/loans.js"
    },
    {
      "src": "/api/loans/(.*)",
      "dest": "/api/loans.js"
    },
    {
      "src": "/api/payments",
      "dest": "/api/payments.js"
    },
    {
      "src": "/api/payments/(.*)",
      "dest": "/api/payments.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_ANON_KEY": "@supabase-anon-key"
  }
}
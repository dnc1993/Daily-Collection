{
  "version": 2,
  "functions": {
    "api/loans.py": {
      "runtime": "python3.9"
    },
    "api/payments.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/loans",
      "dest": "/api/loans.py"
    },
    {
      "src": "/api/loans/(.*)",
      "dest": "/api/loans.py"
    },
    {
      "src": "/api/payments/?(.*)",
      "dest": "/api/payments.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
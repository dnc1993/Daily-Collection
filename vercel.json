{
  "version": 2,
  "builds": [
    {
      "src": "api/loans.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/payments.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/loans",
      "dest": "/api/loans.py"
    },
    {
      "src": "/api/loans/(.*)",
      "dest": "/api/loans.py"
    },
    {
      "src": "/api/payments/?(.*)",
      "dest": "/api/payments.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}